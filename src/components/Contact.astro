---

---

<section id="contact" class="section bg-gray-50 dark:bg-gray-800/50">
  <div class="container-custom">
    <!-- Cal inline embed code begins -->
    <div>
      <h2
        class="mb-6 text-gray-900 dark:text-white text-center text-4xl font-bold"
      >
        Schedule a Free consultation!
      </h2>
    </div>
    <div
      class="animate-slide-up particles-container mb-8"
      style="width:100%;height:20%;overflow:scroll"
      id="my-cal-inline-discovery-call"
    >
    </div>
    <script type="text/javascript">
      (function (C, A, L) {
        let p = function (a, ar) {
          a.q.push(ar);
        };
        let d = C.document;
        C.Cal =
          C.Cal ||
          function () {
            let cal = C.Cal;
            let ar = arguments;
            if (!cal.loaded) {
              cal.ns = {};
              cal.q = cal.q || [];
              d.head.appendChild(d.createElement("script")).src = A;
              cal.loaded = true;
            }
            if (ar[0] === L) {
              const api = function () {
                p(api, arguments);
              };
              const namespace = ar[1];
              api.q = api.q || [];
              if (typeof namespace === "string") {
                cal.ns[namespace] = cal.ns[namespace] || api;
                p(cal.ns[namespace], ar);
                p(cal, ["initNamespace", namespace]);
              } else p(cal, ar);
              return;
            }
            p(cal, ar);
          };
      })(window, "https://app.cal.com/embed/embed.js", "init");
      Cal("init", "discovery-call", { origin: "https://app.cal.com" });

      Cal.ns["discovery-call"]("inline", {
        elementOrSelector: "#my-cal-inline-discovery-call",
        config: { layout: "month_view" },
        calLink: "z-thedev/discovery-call",
      });

      Cal.ns["discovery-call"]("ui", {
        cssVarsPerTheme: {
          light: { "cal-brand": "#176c00" },
          dark: { "cal-brand": "#d7ffe5" },
        },
        hideEventTypeDetails: false,
        layout: "month_view",
      });
    </script>
    <!-- Cal inline embed code ends -->

    <div class="grid md:grid-cols-2 gap-12">
      <div class="slide-up">
        <span
          class="inline-block px-4 py-1 rounded-full bg-secondary-100 dark:bg-secondary-900 text-secondary-600 dark:text-secondary-300 font-medium text-sm mb-4"
        >
          Let's Build Together
        </span>

        <h2 class="mb-6 text-gray-900 dark:text-white">
          Prefer the written word?
        </h2>
        <p class="text-gray-600 dark:text-gray-300 mb-8">
          Whether you need help with a new website, you want to update a legacy
          site or are looking for consulting services, we're here to help you
          succeed with modern web technologies.
        </p>

        <div class="space-y-6">
          <div class="flex items-start">
            <div class="shrink-0 mt-1">
              <svg
                class="w-6 h-6 text-secondary-500"
                fill="none"
                stroke="currentColor"
                viewBox="0 0 24 24"
                xmlns="http://www.w3.org/2000/svg"
              >
                <path
                  stroke-linecap="round"
                  stroke-linejoin="round"
                  stroke-width="2"
                  d="M3 8l7.89 5.26a2 2 0 002.22 0L21 8M5 19h14a2 2 0 002-2V7a2 2 0 00-2-2H5a2 2 0 00-2 2v10a2 2 0 002 2z"
                ></path>
              </svg>
            </div>
            <div class="ml-4">
              <h3 class="text-lg font-semibold text-gray-900 dark:text-white">
                Email Us
              </h3>
              <p class="text-gray-600 dark:text-gray-300">
                support@pnwdigitalsolutions.com
              </p>
            </div>
          </div>

          <div class="flex items-start">
            <div class="shrink-0 mt-1">
              <svg
                class="w-6 h-6 text-secondary-500"
                fill="none"
                stroke="currentColor"
                viewBox="0 0 24 24"
                xmlns="http://www.w3.org/2000/svg"
              >
                <path
                  stroke-linecap="round"
                  stroke-linejoin="round"
                  stroke-width="2"
                  d="M17.657 16.657L13.414 20.9a1.998 1.998 0 01-2.827 0l-4.244-4.243a8 8 0 1111.314 0z"
                ></path>
                <path
                  stroke-linecap="round"
                  stroke-linejoin="round"
                  stroke-width="2"
                  d="M15 11a3 3 0 11-6 0 3 3 0 016 0z"></path>
              </svg>
            </div>
            <div class="ml-4">
              <h3 class="text-lg font-semibold text-gray-900 dark:text-white">
                Based In
              </h3>
              <p class="text-gray-600 dark:text-gray-300">
                Pacific Northwest, USA
              </p>
            </div>
          </div>
        </div>

        <div class="mt-8">
          <h3 class="text-lg font-semibold text-gray-900 dark:text-white mb-4">
            Connect With Us
          </h3>
          <div class="flex space-x-4">
            
            <!-- LinkedIn -->
            <a
              href="https://www.linkedin.com/company/pnwdigitalsolutions/"
              class="text-gray-500 hover:text-primary-600 dark:text-gray-400 dark:hover:text-primary-400 transition-colors"
              aria-label="LinkedIn"
            >
              <svg
                class="w-6 h-6"
                fill="currentColor"
                viewBox="0 0 24 24"
                xmlns="http://www.w3.org/2000/svg"
              >
                <path
                  d="M20.447 20.452h-3.554v-5.569c0-1.328-.027-3.037-1.852-3.037-1.853 0-2.136 1.445-2.136 2.939v5.667H9.351V9h3.414v1.561h.046c.477-.9 1.637-1.85 3.37-1.85 3.601 0 4.267 2.37 4.267 5.455v6.286zM5.337 7.433c-1.144 0-2.063-.926-2.063-2.065 0-1.138.92-2.063 2.063-2.063 1.14 0 2.064.925 2.064 2.063 0 1.139-.925 2.065-2.064 2.065zm1.782 13.019H3.555V9h3.564v11.452zM22.225 0H1.771C.792 0 0 .774 0 1.729v20.542C0 23.227.792 24 1.771 24h20.451C23.2 24 24 23.227 24 22.271V1.729C24 .774 23.2 0 22.222 0h.003z"
                ></path>
              </svg>
            </a>
            <!-- Blog -->
            <a
              href="/blog"
              class="text-gray-500 hover:text-primary-600 dark:text-gray-400 dark:hover:text-primary-400 transition-colors"
              aria-label="Blog"
            >
              <svg
                class="w-6 h-6"
                fill="none"
                stroke="currentColor"
                viewBox="0 0 24 24"
                xmlns="http://www.w3.org/2000/svg"
              >
                <path
                  stroke-linecap="round"
                  stroke-linejoin="round"
                  stroke-width="2"
                  d="M19 20H5a2 2 0 01-2-2V6a2 2 0 012-2h10a2 2 0 012 2v1m2 13a2 2 0 01-2-2V7m2 13a2 2 0 002-2V9a2 2 0 00-2-2h-2m-4-3H9M7 16h6M7 8h6v4H7V8z"
                ></path>
              </svg>
            </a>
          </div>
        </div>
      </div>

      <div class="slide-up" style="animation-delay: 150ms">
        <div class="card p-6 border border-gray-200 dark:border-gray-700">
          <h3 class="text-xl font-semibold mb-6 text-gray-900 dark:text-white">
            Start a Conversation
          </h3>

          <form
            x-data="{ name: '', email: '', message: '', submitted: false, errors: {} }"
          >
            <div class="mb-4">
              <label
                for="name"
                class="block text-sm font-medium text-gray-700 dark:text-gray-300 mb-1"
                >Name</label
              >
              <input
                type="text"
                id="name"
                x-model="name"
                class="w-full px-4 py-2 rounded-lg border border-gray-300 dark:border-gray-600 bg-white dark:bg-gray-800 text-gray-900 dark:text-white focus:outline-hidden focus:ring-2 focus:ring-primary-500 dark:focus:ring-primary-400"
                placeholder="Your name"
              />
              <p
                class="text-red-500 text-sm mt-1"
                x-show="errors.name"
                x-text="errors.name"
              >
              </p>
            </div>

            <div class="mb-4">
              <label
                for="contact-email"
                class="block text-sm font-medium text-gray-700 dark:text-gray-300 mb-1"
                >Email</label
              >
              <input
                type="email"
                id="contact-email"
                x-model="email"
                class="w-full px-4 py-2 rounded-lg border border-gray-300 dark:border-gray-600 bg-white dark:bg-gray-800 text-gray-900 dark:text-white focus:outline-hidden focus:ring-2 focus:ring-primary-500 dark:focus:ring-primary-400"
                placeholder="Your email"
              />
              <p
                class="text-red-500 text-sm mt-1"
                x-show="errors.email"
                x-text="errors.email"
              >
              </p>
            </div>

            <div class="mb-6">
              <label
                for="message"
                class="block text-sm font-medium text-gray-700 dark:text-gray-300 mb-1"
                >Message</label
              >
              <textarea
                id="message"
                x-model="message"
                rows="5"
                class="w-full px-4 py-2 rounded-lg border border-gray-300 dark:border-gray-600 bg-white dark:bg-gray-800 text-gray-900 dark:text-white focus:outline-hidden focus:ring-2 focus:ring-primary-500 dark:focus:ring-primary-400"
                placeholder="Your message"></textarea>
              <p
                class="text-red-500 text-sm mt-1"
                x-show="errors.message"
                x-text="errors.message"
              >
              </p>
            </div>

            <div x-show="!submitted">
              <button
                type="submit"
                class="w-full btn-primary"
                x-on:click.prevent="
                  errors = {};
                  if (!name) errors.name = 'Please enter your name';
                  if (!email) errors.email = 'Please enter your email';
                  else if (!/^[^\s@]+@[^\s@]+\.[^\s@]+$/.test(email)) errors.email = 'Please enter a valid email';
                  if (!message) errors.message = 'Please enter your message';
                  
                  if (Object.keys(errors).length === 0) {
                    submitted = true;
                  }
                "
              >
                Start the Conversation
              </button>
            </div>

            <div
              class="text-center p-6 bg-primary-50 dark:bg-primary-900/30 rounded-lg"
              x-show="submitted"
              x-cloak
            >
              <svg
                class="w-12 h-12 text-primary-500 mx-auto mb-4"
                fill="none"
                stroke="currentColor"
                viewBox="0 0 24 24"
                xmlns="http://www.w3.org/2000/svg"
              >
                <path
                  stroke-linecap="round"
                  stroke-linejoin="round"
                  stroke-width="2"
                  d="M9 12l2 2 4-4m6 2a9 9 0 11-18 0 9 9 0 0118 0z"></path>
              </svg>
              <h3
                class="text-xl font-semibold text-gray-900 dark:text-white mb-2"
              >
                Thanks for Reaching Out!
              </h3>
              <p class="text-gray-600 dark:text-gray-300">
                We appreciate your interest in working with us. We'll review
                your message and get back to you within 48 hours to discuss your
                project.
              </p>
            </div>
          </form>
        </div>
      </div>
    </div>
  </div>
</section>
